{
  "name": "bag",
  "version": "1.0.0",
  "description": "Hack Club's custom inventory system for Slack!",
  "homepage": "https://github.com/hackclub/inventory",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackclub/inventory"
  },
  "main": "index.js",
  "scripts": {
    "start": "tsx index.ts",
    "dev": "tsx watch index.ts",
    "format": "prettier --ignore-path ./.prettierignore --write .",
    "rpc:gen": "npx buf generate proto && cp -r ./gen ./client",
    "db:migrate-dev": "npx prisma migrate dev",
    "db:migrate-deploy": "npx prisma migrate deploy"
  },
  "author": "hackclub",
  "license": "MIT",
  "dependencies": {
    "@bufbuild/buf": "^1.28.1",
    "@bufbuild/protobuf": "^1.5.0",
    "@bufbuild/protoc-gen-es": "^1.5.0",
    "@connectrpc/connect": "^1.1.3",
    "@connectrpc/connect-express": "^1.1.3",
    "@connectrpc/connect-node": "^1.1.4",
    "@connectrpc/protoc-gen-connect-es": "^1.1.4",
    "@hackclub/bag": "^1.0.6",
    "@prisma/client": "^5.7.1",
    "@slack/bolt": "^3.15.0",
    "@slack/oauth": "^2.6.1",
    "@slack/web-api": "^6.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "tsx": "^4.5.0",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
